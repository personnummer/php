{
  "name": "frozzare/personnummer",
  "type": "utility",
  "license": "MIT",
  "description": "Validate Swedish social security numbers",
  "authors": [
    {
      "name": "Fredrik Forsmo",
      "email": "fredrik.forsmo@gmail.com",
      "homepage": "https://github.com/frozzare"
    }
  ],
  "keywords": [
    "utility",
    "personnummer",
    "samordningsnummer",
    "identify",
    "security",
    "swedish",
    "social security number",
    "personal identity number",
    "personal id",
    "co-ordination",
    "coordination"
  ],
  "support": {
    "issues": "https://github.com/frozzare/php-personnummer/issues"
  },
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Frozzare\\Personnummer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Frozzare\\Tests\\Personnummer\\": "tests/"
    }
  },
  "scripts": {
    "lint": "vendor/bin/phpcs --standard=PSR2 src",
    "test": "vendor/bin/phpunit"
  }
}
